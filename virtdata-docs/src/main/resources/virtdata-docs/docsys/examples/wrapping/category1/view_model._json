{
  'version': {{ version }},
  'target': '{{ path.toString}}',
  'paths' : [
  {{# paths }}
    '{{ . }}'{{^-last}},{{/-last}}
  {{/ paths }}
  ],
  'breadcrumbs' : [
  {{# breadcrumbs }}
    '{{ . }}'{{^-last}},{{/-last}}
  {{/ breadcrumbs }}
  ],
  'topics' : {
    {{# topics.list }}
      '{{ name }}': '{{ path }}'{{^-last}},{{/-last}}
    {{/ topics.list }}
  }
}